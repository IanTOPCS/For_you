{
    "name": "C++ Dev Container",
    "dockerComposeFile":[
		"app.yml"
	],
	"service": "cplusplus",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-extension-pack",
                "ms-vscode.cmake-tools"
            ]
        }
    },
    "remoteUser": "root"
}